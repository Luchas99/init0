// Header start
header
  .search-bar.closed
    form
      .input-group.input-group-lg
        input.form-control(type='text', placeholder='Search for...')
        span.input-group-btn
          button.btn.btn-default.search-bar-toggle(type='button')
            i.ti-close
  .brand.pull-left
    a.logo(href='index.html')
      i.ti-underline
      h2 MEGA
        span app
  a.sidebar-toggle.pull-left.icon(href='javascript:;', role='button')
    i.ti-menu.text-muted
  form.mt-15.mb-15.pull-left.hidden-xs.hidden-sm
    .form-group.has-feedback.mb-0
      input.form-control.rounded(type='text', aria-describedby='inputSearchFor', placeholder='Search for...')
      span.ti-search.form-control-feedback(aria-hidden='true')
      span#inputSearchFor.sr-only (default)
  ul.newsticker.list-unstyled.ml-15.mb-0.pull-left.visible-lg
    li.fw-500 You 
      span.text-danger recommended
      |  Karen Ortega.
    li.fw-500 You 
      span.text-info followed
      |  Olivia Williamson.
    li.fw-500 New product 
      span.text-success Microsoft Lumia 950XL
    li.fw-500 New order 
      span.text-success #021930217965
  ul.notification-bar.list-inline.pull-right
    li.visible-xs.visible-sm
      a.icon.search-bar-toggle(href='javascript:;', role='button')
        i.ti-search.text-muted
    li.hidden-xs
      a.icon.fullscreen-toggle(href='javascript:;', role='button')
        i.ti-fullscreen.text-muted
    li.dropdown
      a#dropdownMenu1.dropdown-toggle.bubble.icon(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
        i.ti-save.text-muted
        span.badge.bg-danger 3
      .dropdown-menu.dropdown-menu-right.dm-medium.fs-12.animated.fadeInDown(aria-labelledby='dropdownMenu1')
        h5.dropdown-header You have 3 file downloads
        ul.media-list
          li.media
            a(href='javascript:;')
              .media-left
                i.ti-music-alt.media-object.mo-sm.img-circle.bg-info.text-center
              .media-body
                h6.media-heading Music.mp3
                .progress.progress-xs.mb-5
                  .progress-bar.progress-bar-info(role='progressbar', aria-valuenow='60', aria-valuemin='0', aria-valuemax='100', style='width: 60%;')
                    span.sr-only 60% Complete
                p.text-muted.mb-0 1.3 of 4MB - 685KB/sec - 4 sec
          li.media
            a(href='javascript:;')
              .media-left
                i.ti-video-clapper.media-object.mo-sm.img-circle.bg-danger.text-center
              .media-body
                h6.media-heading Video.mp4
                .progress.progress-xs.mb-5
                  .progress-bar.progress-bar-success(role='progressbar', aria-valuenow='100', aria-valuemin='0', aria-valuemax='100', style='width: 100%;')
                    span.sr-only 100% Complete
                p.text-muted.mb-0 Completed - 5 minutes ago
          li.media
            a(href='javascript:;')
              .media-left
                i.ti-zip.media-object.mo-sm.img-circle.bg-warning.text-center
              .media-body
                h6.media-heading Copy.zip
                .progress.progress-xs.mb-5
                  .progress-bar(role='progressbar', aria-valuenow='45', aria-valuemin='0', aria-valuemax='100', style='width: 45%;')
                    span.sr-only 45% Complete
                p.text-muted.mb-0 650 of 1451MB - 721KB/sec - 20 min
        .dropdown-footer.text-center.p-10
          a.fw-500.text-muted(href='javascript:;') See all file downloads
    li.dropdown
      a#dropdownMenu2.dropdown-toggle.bubble.icon(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
        i.ti-world.text-muted
        span.badge.bg-danger 4
      .dropdown-menu.dropdown-menu-right.dm-medium.fs-12.animated.fadeInDown(aria-labelledby='dropdownMenu2')
        h5.dropdown-header You have 4 notifications
        ul.media-list
          li.media
            a(href='javascript:;')
              .media-left.avatar
                img.media-object.img-circle(src=build+'images/users/02.jpg', alt='')
                span.status.bg-success
              .media-body
                h6.media-heading Mark Barnett
                p.text-muted.mb-0 Sent you a new message
              .media-right.text-nowrap
                time.fs-11(datetime='2015-12-10T20:50:48+07:00') 9 mins
          li.media
            a(href='javascript:;')
              .media-left.avatar
                img.media-object.img-circle(src=build+'images/users/11.jpg', alt='')
                span.status.bg-danger
              .media-body
                h6.media-heading Alexander Gilbert
                p.text-muted.mb-0 Sent you a new email
              .media-right.text-nowrap
                time.fs-11(datetime='2015-12-10T20:42:40+07:00') 15 mins
          li.media
            a(href='javascript:;')
              .media-left.avatar
                img.media-object.img-circle(src=build+'images/users/12.jpg', alt='')
                span.status.bg-success
              .media-body
                h6.media-heading Amanda Collins
                p.text-muted.mb-0 You have 8 unread messages
              .media-right.text-nowrap
                time.fs-11(datetime='2015-12-10T20:35:35+07:00') 22 mins
          li.media
            a(href='javascript:;')
              .media-left.avatar
                img.media-object.img-circle(src=build+'images/users/13.jpg', alt='')
                span.status.bg-warning
              .media-body
                h6.media-heading Christian Lane
                p.text-muted.mb-0 Commented on your post
              .media-right.text-nowrap
                time.fs-11(datetime='2015-12-10T20:27:48+07:00') 30 mins
        .dropdown-footer.text-center.p-10
          a.fw-500.text-muted(href='javascript:;') See all notifications
    li
      a.right-sidebar-toggle.bubble.icon(href='javascript:;', role='button')
        i.ti-comment-alt.text-muted
        span.dot.bg-danger
    li
      a.icon(href='login.html', role='button')
        i.ti-power-off.text-muted
// Header end